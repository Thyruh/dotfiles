setxkbmap -option caps:ctrl_modifier
setxkbmap -layout us,ru -option grp:alt_shift_toggle


alias C++="cd && cd Architect/C++ && emacsclient -c new.cpp"
alias py="cd && cd Architect/Python && emacsclient -c new.py"
alias hsk="cd && cd Architect/Haskell && emacsclient -c new.hs"
alias cls="clear && cd"
alias ..="cd .."
alias ...=".. && .."
alias 2.="... && .."
alias 3.="2. && .."
alias arc="cd && cd Architect && emacsclient -c"
alias sn="sudo shutdown now"
alias sr="sudo reboot"
alias qwer="setxkbmap real_programmers_dvorak"
alias aoeu="setxkbmap us"
alias bash="emacsclient -c ~/.bashrc && source ~/.bashrc"
export PATH="/usr/local/bin:$PATH"

